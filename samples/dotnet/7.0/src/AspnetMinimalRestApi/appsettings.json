{
  "AllowedHosts": "*",
  "Application": {
    "IsHttpsRedirectionEnabled": true,
    "IsSwaggerEnabled": false
  },
  "Logging": {
    "Console": {
      "IncludeScopes": false,
      "LogLevel": {
        "Microsoft.EntityFrameworkCore": "Information",
        "Default": "Information"
      }
    },
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  }
}
